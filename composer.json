{
  "name": "cleverage/eav-manager-akeneo-product-bundle",
  "description": "Interface to interact with Akeneo by the Clever Data Manager and Akeneo API",
  "keywords": [
    "eav-manager",
    "akeneo",
    "client",
    "api"
  ],
  "homepage": "https://github.com/cleverage/eav-manager-akeneo-product-bundle",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Vincent Chalnot",
      "email": "vchalnot@clever-age.com",
      "homepage": "http://chalnot.fr",
      "role": "Lead Developer"
    },
    {
      "name": "Fabien Salles",
      "email": "fsalles@clever-age.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "CleverAge\\EAVManager\\AkeneoProductBundle\\": ""
    }
  },
  "require": {
    "php": ">=7.1",
    "cleverage/eav-manager": "dev-v2.0-dev",
    "cleverage/process-bundle": "dev-v2.0-dev",
    "akeneo/api-php-client": "^3.0",
    "php-http/client-implementation": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.4",
    "php-http/guzzle6-adapter": "^1.1"
  },
  "suggest": {
    "php-http/guzzle6-adapter": "In order to use Guzzle v6 as the HTTP client"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
